{
	"extends": [
		"plugin:unicorn/recommended",
		"next/core-web-vitals",
		"next/typescript"
	],
	"plugins": ["unicorn"],
	"ignore_patterns": ["dist/**"],
	"globals": {
		"Global1": "readonly"
	},
	"rules": {
		"eqeqeq": "error"
	},
	"overrides": [
		{
			"files": ["tests/**"],
			"rules": {
				"eqeqeq": "off"
			}
		}
	]
}
